/* harmony import */ var _angular_forms__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/forms */ "./node_modules/@angular/forms/fesm2015/forms.js");
/* harmony import */ var _core_authentication_auth_service__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ../../../core/authentication/auth.service */ "./src/app/core/authentication/auth.service.ts");
/* harmony import */ var _content_services_usuario_service__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ../../../content/services/usuario.service */ "./src/app/content/services/usuario.service.ts");
/* harmony import */ var _core_services_message_service__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! ../../../core/services/message.service */ "./src/app/core/services/message.service.ts");
/* harmony import */ var _content_models_perfil_usuario_enum__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! ../../../content/models/perfil-usuario.enum */ "./src/app/content/models/perfil-usuario.enum.ts");







let InfoUsuarioComponent = class InfoUsuarioComponent {
    constructor(authService, formBuilder, usuarioService, messageService) {
        this.authService = authService;
        this.formBuilder = formBuilder;
        this.usuarioService = usuarioService;
        this.messageService = messageService;
        this.usuarioService.list().subscribe((users) => {
            this.usuarios = users;
        });
    }
    ngOnInit() {
        this.usuario = this.authService.getauthData();
        this.form = this.formBuilder.group({
            nome: [this.usuario.nome, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].maxLength(50), _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required])],
            email: [{ value: this.usuario.email, disabled: true }],
            senha: [this.usuario.senha, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].minLength(8), _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required])],
            perfil: [{
                    value: this.usuario.perfil,
                    disabled: this.usuario.perfil.nome === _content_models_perfil_usuario_enum__WEBPACK_IMPORTED_MODULE_6__["PerfilUsuarioEnum"].VISITANTE
                }, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            ativo: [this.usuario.ativo, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required],
            telefone: [this.usuario.telefone, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].maxLength(16),
                    _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].minLength(10)])],
            cep: [this.usuario.cep, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].minLength(8),
                    _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].maxLength(8)])],
            endereco: [this.usuario.endereco, _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].compose([_angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].maxLength(50)])],
        });
    }
    salvar() {
        this.usuarioService.list().subscribe((usuarios) => {
            const users = usuarios.filter(user => user.perfil.nome === _content_models_perfil_usuario_enum__WEBPACK_IMPORTED_MODULE_6__["PerfilUsuarioEnum"].ADMINISTRADOR && user.ativo === true);
            if (users.length <= 1 &&
                this.usuario.perfil.nome === _content_models_perfil_usuario_enum__WEBPACK_IMPORTED_MODULE_6__["PerfilUsuarioEnum"].ADMINISTRADOR && (this.form.value.perfil === _content_models_perfil_usuario_enum__WEBPACK_IMPORTED_MODULE_6__["PerfilUsuarioEnum"].VISITANTE
                || this.form.value.ativo === false)) {
                this.messageService.showMessageInfo('ImpossÃ­vel salvar as alteraÃ§Ãµes, pois deve haver ao menos um administrador.');
            }
            else {
                const user = this.getInformacoes(this.form.value);
                this.usuarioService.update(user.email, user).subscribe(data => {
                    this.authService.updateInfo(user);
                    this.messageService.showMessageSucessfull('Dados alterados com sucesso!');
                }, error => {
                    this.messageService.showMessageError('Erro ao atualizar as informaÃ§Ãµes...');
                });
            }
        });
    }
    getInformacoes(formValues) {
        return {
            perfil: formValues.perfil === undefined ? this.usuario.perfil : formValues.perfil,
            email: this.usuario.email,
            nome: formValues.nome,
            ativo: formValues.ativo,
            senha: formValues.senha,
            cep: formValues.cep,
            endereco: formValues.endereco,
            telefone: formValues.telefone
        };
    }
};
InfoUsuarioComponent.ctorParameters = () => [
    { type: _core_authentication_auth_service__WEBPACK_IMPORTED_MODULE_3__["AuthService"] },
    { type: _angular_forms__WEBPACK_IMPORTED_MODULE_2__["FormBuilder"] },
    { type: _content_services_usuario_service__WEBPACK_IMPORTED_MODULE_4__["UsuarioService"] },
    { type: _core_services_message_service__WEBPACK_IMPORTED_MODULE_5__["MessageService"] }
];
InfoUsuarioComponent = tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([
    Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Component"])({
        selector: 'app-info-usuario',
        template: tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(/*! raw-loader!./info-usuario.component.html */ "./node_modules/raw-loader/dist/cjs.js!./src/app/shared/components/info-usuario/info-usuario.component.html")).default,
